{
    // 左上角的标题（也是首页图像下面的一级标题）
    "title": "Notes",
    // 首页头像下面标题的解释
    "description": "知识速查（了不起的个人工具库）",
    // 右上角 跳转GitHub的地址（写一个GitHub项目地址）
    "repo": "https://github.com/itz2019/vuepress-es",

    // 设置侧边栏是否全部默认展开（一般官方介绍经常使用这个）
    "collapsable": true, // 注意：false 是展开，true 是不展开
    // 代码块是否显示行号
    "lineNumbers": false,

    // 文件夹展示顺序（有需求就进行设置，不仅可以设置需要显示的文件夹，还能设置展示的顺序，这里可能有点绕）
    "folderShowOrder": [
        {
            "title": "1.quickcheck",
            "alias": "速查",
            "childrenFolder": ["0.重构内容目录（之后会删除）", "1.前端", "2.后端", "3.运维", "4.计算机基础知识", "5.其他", "6.面试题", ]
        },
        {
            "title": "2.note",
            "alias": "前端体系",
            "childrenFolder": ["1.HTML", "2.CSS", "3.JavaScript", "4.jQuery及其周边库", "5.1MVVM框架（Vue）", "5.2MVVM框架（React）", "6.移动端", "7.Node", "8.0网络", "8.1前端工程化", "8.2 前端安全", "8.3前端性能优化", "8.4前端代码质量", "8.5运维", "8.6前端架构", "9.项目", ]
        },
        {
            "title": "3.other",
            "alias": "其他",
            "childrenFolder": ["0.项目方向指南", "1.每日规划", "2.健身", "3.养猫", "4.Switch", ]
        },
        
    ]
}